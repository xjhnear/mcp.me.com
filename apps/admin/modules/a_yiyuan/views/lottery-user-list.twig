<!--{%extends "base-layout.twig"%}-->
<!--{%block main_content%}-->
<div class="row-fluid">
    <div class="span12">
        <div class="box box-color box-bordered">
            <div class="box-title">
                <h3>
                    <i class="icon-table"></i>
                    参与用户
                </h3>
                <div class="actions">
                    <form action="<!--{{url('a_yiyuan/lottery/user-list')}}-->" class="">
                        <input type="hidden" name="drawId" value="<!--{{ search.drawId }}-->"/>
                        <table class="table-filter">
                            <tr>
                                <td class="span2">
                                    <p class="span3" style="margin-bottom:0px">开始时间</p>
                                    <div class="input-append date datepick span9">
                                        <input name="startTime" type="text" class="span9" value="<!--{{ search.startTime }}-->">
                                        <span class="add-on"><i class="icon-calendar"></i></span>
                                    </div>
                                </td>
                                <td class="span2">
                                    <p class="span3" style="margin-bottom:0px">结束时间</p>
                                    <div class="input-append date datepick span9">
                                        <input name="endTime" type="text" class="span9" value="<!--{{ search.endTime }}-->">
                                        <span class="add-on"><i class="icon-calendar"></i></span>
                                    </div>
                                </td>
                                <td class="span1">
                                    <input type="text" placeholder="参与号码" class="span11" value="<!--{{ search.partakeNum }}-->" name="partakeNum">
                                </td>
                                <td class="span1">
                                    <input type="text" placeholder="用户uid" class="span11" value="<!--{{ search.userId }}-->" name="userId">
                                </td>
                                <td class="span1">
                                    <button class="btn btn-teal">
                                        <i class="icon-search"></i>
                                        搜索
                                    </button>
                                </td>
                            </tr>
                        </table>
                    </form>
                </div>
            </div>
            <div class="box-content nopadding">
                <table class="table table-hover table-nomargin">
                    <thead>
                    <tr>
                        <th class="col-400">参与用户</th>
                        <th class="col-400">参与时间</th>
                        <th class="col-400">参与号码</th>
                    </tr>
                    </thead>
                    <tbody>
                    <!--{%for item in list%}-->
                    <tr>
                        <td><!--{{item.userhtml}}--></td>
                        <td><!--{{item.partakeTime}}--></td>
                        <td><!--{{item.partakeNum}}--></td>
                    </tr>
                    <!--{%endfor%}-->
                    </tbody>
                </table>
                <div class="pagelink">
                    <!--{{pagelinks}}-->
                </div>
            </div>
        </div>
    </div>
</div>
<!--{%endblock%}-->

<!--{%block footer_js%}-->
