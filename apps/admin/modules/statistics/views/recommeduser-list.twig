<!--{%extends "sub-layout-statistics.twig"%}-->
<!--{%block main_content%}-->
<div class="row-fluid">
	<div class="span12">
	    <div class="box box-color box-bordered">
			<div class="box-title">
				<h3>
					<i class="icon-table"></i>
					用户列表
				</h3>
			</div>
			<div class="box-content nopadding">
			    <table class="table table-hover table-nomargin">
			        <thead>
						<tr>
						    <th class="col">
							</th>
						    <th class="col">ID</th>
							<th class="col">用户名</th>
							<th class="col">Email</th>
							<th class="col">注册时间</th>
							<th class="col">游币</th>
							<th class="col">时间</th>
						</tr>
					</thead>
					<tbody>
					    <!--{%for item in datalist%}-->
						<tr>
						    <td></td>
							<td><!--{{item.uid}}--></td>
							<td><!--{{item.nickname}}--></td>
							<td><!--{{item.email}}--></td>
							<td><!--{{item.dateline|date('Y-m-d H:i:s')}}--></td>
							<td><!--{{item.score}}--></td>
							<td><!--{{item.ctime|date('Y-m-d')}}--></td>
						</tr>
						<!--{%endfor%}-->
					</tbody>
			    </table>
			    <div class="pagelink">
			        <!--{{pagelinks}}-->
			    </div>
			</div>
		</div>
	</div>
</div>
<!--{%endblock%}-->
