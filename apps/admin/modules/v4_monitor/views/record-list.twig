<!--{%extends "sub-layout-shop.twig"%}-->
<!--{%block main_content%}-->
<div class="row-fluid">
	<div class="span12">
		<div class="box box-color box-bordered">
			<div class="box-title">
				<h3>
					<i class="icon-table"></i>
					主线程管理
					<span class="badge badge-warning"><!--{{totalcount}}--></span>
				</h3>
				<div class="actions">
					<form action="<!--{{url('/v4monitor/record/list')}}-->">
						<table class="table-filter">
							<tr>
	                            <td>
	                            	<input name="urlName"  placeholder="请填写主线程URL" style="width:177px;" type="text" class="input-small" value="<!--{{urlName}}-->" >
	                            </td>
	                            <td>
	                            	<input name="baseId"  placeholder="请填写主线程标识" style="width:177px;" type="text" class="input-small" value="<!--{{baseId}}-->" >
	                            </td>
								<td>
									时间
									<div class="input-append date " >
										<input name="addtime" onclick="laydate({istime: true, format: 'YYYY-MM-DD hh:mm:ss'})" type="text" class="input-medium datetimepicker" id="date_timepicker_start" value="<!--{{addtime}}-->" readonly>
										<span class="add-on"><i class="icon-calendar"></i></span>
									</div>

								</td>
                        	<td><button class="btn btn-teal"><i class="icon-search"></i>搜索</button></td>
                        </tr>
                     </table>
					</form>	
				</div>
			</div>
			<div class="box-content nopadding">
				<table class="table table-hover table-nomargin">
					<thead>
					<tr>
						<!-- <th class="col-60">ID</th> -->
						<th class="col-100">主线程标识/主线程URL</th>
						<th class="col-60">URL类型</th>
						<th class="col-60">执行时间(毫秒)</th>
						<th class="col-60">网络环境</th>
						<th class="col-80">添加时间</th>
						<th class="col-80">IDFA</th>
						<th class="col-80">UID</th>
						<th class="table-btn-group">操作</th>
					</tr>
					</thead>
					<tbody>
					<!--{% for key,item in datalist%}-->
					<tr id="tr_<!--{{item.id}}-->">
						<td><!--{{item.baseId}}--><br/><!--{{item.urlName}}--></td>
						<td><!--{{item.urltype}}--></td>
						<td><!--{{item.time}}--></td>
						<td><!--{{item.iswifi}}--></td>
						<td><!--{{item.addTime}}--></td>
						<td><!--{{item.deviceId}}--></td>
						<td><!--{{item.uid}}--></td>
						<td>
							<a href="<!--{{url('v4monitor/record/detail',{baseId:item.baseId})}}-->" class="btn btn-primary">详情</a>
						</td>
					</tr>
					<!--{%endfor%}-->
					</tbody>
				</table>
				<div class="pagelink">
					<!--{{pagelinks}}-->
				</div>
			</div>
		</div>
	</div>
</div>

<!--{%endblock%}-->
<!--{%block footer_js%}-->
<script src="/static/js/laydate/laydate.js"></script>
<script src="/static/js/ajaxfileupload.js"></script>
<script>
    $('[data-toggle="popover"]').popover();
</script>
<!--{%endblock%}-->