<!--{%extends "base-layout.twig"%}-->
<!--{%block main_content%}-->
<div class="row-fluid">
	<div class="span12">
	    <div class="box box-color box-bordered">
			<div class="box-title">
				<h3>
					<i class="icon-table"></i>
					待入库游戏				
				</h3>
				
				</div>
			<div class="box-content nopadding">
			    <table class="table table-hover table-nomargin">
			        <thead>
						<tr>
						    <th class="col-60">ID</th>
						    <th class="col-60">图标</th>
							<th class="col-150">游戏</th>
							<th class="col-150">包名</th>		
							<th class="col-60">匹配次数</th>					
							<th class="col-80">游戏更新时间</th>
							<th class="table-btn-group">操作</th>
						</tr>
					</thead>
					<tbody>
					    <!--{%for item in datalist%}-->
						<tr>
							<td><!--{{item.ID}}--></td>
							<td class="user-avatar"><img src="<!--{{item.APP_ICON}}-->" class="" /></td>
							<td><!--{{item.APP_NAME}}--></td>
							<td><!--{{item.APK_PACKAGENAME}}--></td>	
							<td><!--{{match_pkg[item.APK_PACKAGENAME]|default(0)}}--></td>						
							<td><!--{{item.APP_RELEASE_TIME|date('Y-m-d')}}--></td>
							<td>							   
							    <a href="<!--{{url('a_game/games/game-add',{'gid':item.id})}}-->" class="btn btn-primary">入库</a>
							</td>
						</tr>
						<!--{%endfor%}-->
					</tbody>
			    </table>
			    <div class="pagelink">
			        <!--{{pagelinks}}-->
			    </div>
			</div>
		</div>
	</div>
</div>

<!--{%endblock%}-->